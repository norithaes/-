#include <stdio.h>

int hv[100];
int by[100];
int n;

void ob();
void id(int id);

int main() {
    int ID;

    ob();  

    printf("ID 입력: ");
    scanf_s("%d", &ID);

    id(ID); 

    return 0;
}

void ob() {
    printf("상품 개수(종류) 입력\n");
    scanf_s("%d", &n);

    printf("상품 별 입고수량 입력\n");
    for (int i = 0; i < n; i++) {
        scanf_s("%d", &hv[i]);
    }

    printf("상품 별 판매수량 입력\n");
    for (int i = 0; i < n; i++) {
        scanf_s("%d", &by[i]);
    }
}

void id(int id) {
    int stock[100];

    for (int i = 0; i < n; i++) {
        stock[i] = hv[i] - by[i];
        if (stock[i] < 0) stock[i] = 0;
    }


    for (int i = 0; i < n; i++) {
        printf("%d ", stock[i]);
    }
    printf("\n");
}
